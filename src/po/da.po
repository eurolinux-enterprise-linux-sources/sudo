msgid ""
"\n"
"Options:\n"
msgstr ""
"\n"
"Tilvalg:\n"

#: src/parse_args.c:680
msgid "use a helper program for password prompting"
msgstr "brug et hjælpeprogram for indhentelse af adgangskode"

#: src/parse_args.c:683
msgid "use specified BSD authentication type"
